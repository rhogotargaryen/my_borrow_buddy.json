<%= form_for(@user) do |f|%>
  <%= f.label :name %></br>
  <%= f.text_field :name %></br>
  <%= f.label :username %></br>
  <%= f.text_field :username %></br>
  <%= f.label :password %></br>
  <%= f.text_field :password %></br>
  <%= f.label :email %></br>
  <%= f.text_field :email %></br>
  <%= f.submit %></br>
<% end %>